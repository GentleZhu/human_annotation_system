doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible" ,content="IE=edge")
		meta(name="viewport",content="width=device-width, initial-scale=1")
		script(type="text/javascript" src="/js/jquery.min.js")
		link(rel="stylesheet",type="text/css",href="/css/bootstrap.min.css")
		link(rel="stylesheet",type="text/css",href="/css/index.css")
		title Login
	body
		h1(align="center") Human augmented annotation system
		div(style="top:20%" class="col-md-6 col-md-offset-3" )
			form(method="post" action="/login" role="form" class="form-horizontal")
				div(class="form-group",width="100px")
					label(for="InputName") User Name
					input(type="text",class="form-control",id="InputName",name="InputName",placeholder="Username")
				button(type="submit" class="btn btn-default") Continue